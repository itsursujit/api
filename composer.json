{
    "name": "itsursujit/api",
    "description": "API Key Middleware",
    "type": "library",
    "license": "MIT",
    "version": "0.1",
    "authors": [
        {
            "name": "Sujit Baniya",
            "email": "itsursujit@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.0",
        "zendframework/zend-diactoros": "^1.7"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.2"
    },
    "autoload": {
        "psr-4" : {
            "Sujit\\Api\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sujit\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml --coverage-text --coverage-clover=coverage.clover --debug"
    }
}
